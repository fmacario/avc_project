{
  "name": "angular-seed-cli-admin",
  "version": "0.0.6",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "./node_modules/.bin/ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "./node_modules/.bin/ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "sw": "./node_modules/.bin/sw-precache --verbose --root=www --config=sw-precache-config.js",
    "build": "./node_modules/.bin/ng build",
    "build.dev": "npm run build -- --dev & npm run sw",
    "build.prod": "npm run build -- --prod & npm run sw",
    "build.prod.exp": "npm run build -- --prod --aot & npm run sw",
    "build.watch": "npm run build -w",
    "serve.browser": "cordova emulate browser -- --port=5555",
    "cordova.livesync": "cordova run -- --live-reload",
    "serve": "cordova serve 5555",
    "postinstall": "mkdir www & cordova prepare",
    "start.cordova": "npm run build & cordova serve",
    "start.windows": "npm run build & cordova run windows",
    "install.windows": "cordova platform rm windows & cordova platform add windows --save",
    "start.ubuntu": "npm run build & cordova run --device",
    "install.ubuntu": "cordova platform rm ubuntu & cordova platform add ubuntu --save",
    "start.ios": "npm run build & cordova run ios",
    "install.ios": "cordova platform rm ios & cordova platform add ios --save",
    "start.osx": "npm run build & cordova run",
    "install.osx": "cordova platform rm osx & cordova platform add osx --save",
    "start.android": "npm run build & cordova run android",
    "install.android": "cordova platform rm android & cordova platform add android --save",
    "start.browser": "npm run build & cordova run browser -- --port=5555",
    "install.browser": "cordova platform rm browser & cordova platform add browser --save"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.2.1",
    "@types/lodash": "ts2.0",
    "angularfire2": "^2.0.0-beta.8",
    "browser-sync": "2.17.5",
    "cheerio": "0.22.0",
    "core-js": "2.4.1",
    "csp-parse": "0.0.2",
    "elementtree": "0.1.6",
    "firebase": "^3.6.10",
    "glob": "7.1.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "moment": "2.15.2",
    "ng2-bootstrap": "1.1.14",
    "ng2-charts": "1.4.0",
    "npm": "3.10.9",
    "oidc-client": "1.2.1-beta.3",
    "plist": "2.0.1",
    "q": "1.4.1",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "1.1.2",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.2.1",
    "@types/jasmine": "2.5.37",
    "@types/lodash": "^4.14.50",
    "@types/node": "6.0.46",
    "angular-cli": "1.0.0-beta.20-4",
    "codelyzer": "1.0.0-beta.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "protractor": "4.0.9",
    "sw-precache": "^4.2.3",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3"
  }
}
